    "str=\"teri maa gu\"\n",
    "\n",
    "vowels=set([\"a\",\"e\",\"i\",\"o\",\"u\"])\n",
    "a=set(str.lower())\n",
    "b=a.intersection(vowels)\n",
    "\n",
    "for i in b:\n",
    "    count= str.lower().count(i)\n",
    "    print(f\"The String contains {i} :{count} Times\")"
   ]
