   ],
   "source": [
    "def circulate(a,n):\n",
    "    circulate=a[n:]+a[:n]\n",
    "    return circulate\n",
    "a=[1,2,3,4,5]\n",
    "n=2\n",
    "print(f\"Intial A{a}\\nn:{n}\\nCirculated with n:{circulate(a,n)}\")"
   ]
  }
